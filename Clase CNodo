package Enunciado02;

/**
 *
 * @author Fabrissio
 */
public class CNodo {
    private int codigo;
    private String nombre;
    private double nota;
    private CNodo siguiente;

    public CNodo(int codigo, String nombre, double nota) {
        this.codigo = codigo;
        this.nombre = nombre;
        this.nota = nota;
    }
    
    public CNodo (){
        codigo = 0;
        nombre = "";
        nota = 0;
        siguiente = null;
    }

    public int getCodigo() {
        return codigo;
    }

    public void setCodigo(int codigo) {
        this.codigo = codigo;
    }

    public String getNombre() {
        return nombre;
    }

    public void setNombre(String nombre) {
        this.nombre = nombre;
    }

    public double getNota() {
        return nota;
    }

    public void setNota(double nota) {
        this.nota = nota;
    }

    public CNodo getSiguiente() {
        return siguiente;
    }

    public void setSiguiente(CNodo siguiente) {
        this.siguiente = siguiente;
    }    
    
}
